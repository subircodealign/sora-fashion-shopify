{% render 'nav-product' %}

<script src="{{ 'pagination.js' | asset_url }}" defer></script>

{% schema %}
{
  "name": "Woman Paginated Product",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Section Heading",
      "default": "Latest Products"
    },
    {
      "type": "range",
      "id": "per_page",
      "label": "Products per Page",
      "min": 3,
      "max": 50,
      "step": 1,
      "default": 12
    },
    {
      "type": "collection",
      "id": "collection",
      "label": "Product Collection (optional)"
    },
    {
      "type": "product_list",
      "id": "product_list",
      "label": "Manual Products (optional)",
      "limit": 50
    }
  ],
    "blocks": [
    {
      "type": "category_link",
      "name": "Category Link",
      "settings": [
        {
          "type": "text",
          "id": "label",
          "label": "Link Label",
          "default": "Man"
        },
        {
          "type": "url",
          "id": "link",
          "label": "Link URL"
        }
      ]
    },
  
  ],
  "presets": [
    {
      "name": "Woman Paginated Products",
      "category": "Products"
    }
  ]
}
{% endschema %}
